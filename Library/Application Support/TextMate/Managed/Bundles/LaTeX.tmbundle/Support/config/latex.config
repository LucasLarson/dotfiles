{
	commands = {
		it = '\\textit{$1} ';
		bf = '\\textbf{$1} ';
		tt = '\\texttt{$1} ';
		sf = '\\textsf{$1} ';
		sc = '\\textsc{$1} ';
		e  = '\\emph{$1} ';
		em = '\\emph{$1} ';
		fc = '\\footcite{$1} ';
		fn = '\\footnote{$1} ';
		cli = '\\lstinline{$1} ';
		ds = '\\displaystyle';
		dis = '\\displaystyle';
		
		hf = '\\hfill';
		vf = '\\vfill';
		
		dc = '\\documentclass{${1:article}}\n';
		nc = '\\newcommand{$1}{$2}';
		rnc = '\\renewcommand{$1}{$2}';
		usep = '\\usepackage{$1}';
		geo = '\\usepackage[left=${1:2cm},right=${2:2cm},top=${3:1cm},bottom=${4:2cm}]{geometry}';
		
		sec = '\\section{$1}';
		sub = '\\subsection{$1}';
		ssub = '\\subsubsection{$1}';
		subs = '\\subsubsection{$1}';
		cha = '\\chapter{$1}';
		par = '\\paragraph{$1}';
		subp = '\\subparagraph{$1}';

		
		fr = '\\frac{${1:num}}{$2} $0';
		frac = '\\frac{${1:num}}{$2} $0';
		b = '\\binom{$1}{$2} $0';
		bin = '\\binom{$1}{$2} $0';
		l   = '\\lim_{$1} $0';
		lim = '\\lim_{$1} $0';
		s = '\\sum_\{${1:bot}\}^{$2} $0';
		sum = '\\sum_\{${1:bot}\}^{$2} $0';
		p = "\\prod_\{${1:bot}\}^{$2} $0";
		prod = "\\prod_\{${1:bot}\}^{$2} $0";

		put = "\\put(${1:0},${2:0}){$0}";
		mp = "\\multiput(${1:0},${2:0})(${3:5},${4:0}){${5:2}}{$0}";
		mput = "\\multiput(${1:0},${2:0})(${3:5},${4:0}){${5:2}}{$0}";
		multiput = "\\multiput(${1:0},${2:0})(${3:5},${4:0}){${5:2}}{$0}";
		line = "\\line(${1:0},${2:0}){${3:0}}{$0}";
		cir = "\\circle${1:[*]}{$0}";
		circle = "\\circle${1:[*]}{$0}";
	};
	commands_beamer = {
		un = "\\uncover<${1:+-}>{$2} ";
		uncover = "\\uncover<${1:+-}>{$2} ";
		o = "\\only<${1:+-}>{$2} ";
		on = "\\only<${1:+-}>{$2} ";
		only = "\\only<${1:+-}>{$2} ";
		os = "\\onslide<${1:+-}>{$2} ";
		ons = "\\onslide<${1:+-}>{$2} ";
		onslide = "\\onslide<${1:+-}>{$2} ";
		un = "\\uncover<${1:+-}>{$2} ";
		uncover = "\\uncover<${1:+-}>{$2} ";
		a = "\\alert<${1:+-}>{$2} ";
		al = "\\alert<${1:+-}>{$2} ";
		alert = "\\alert<${1:+-}>{$2} ";
		v = "\\visible<${1:+-}>{$2} ";
		vis = "\\visible<${1:+-}>{$2} ";
		visible = "\\visible<${1:+-}>{$2} ";
	};
	environments = {
		itemize = {
			triggers = ( 'it', 'item', 'itemize');
			content = "\t\\item $0";
		};
		enumerate = {
			triggers = ( 'en', 'enum', 'enumerate');
			content = "\t\\item $0";
		};
		description = {
			triggers = ( 'desc', 'description');
			content = "\t\\item[$1] $0";
		};
		document = {
			triggers = ( doc, document );
		};
		equation = {
			triggers = ( eq, eqn, equation );
		};
		eqnarray = {
			triggers = ( eqa, eqnarray );
		};
		'equation*' = {
			triggers = ( 'eq*', 'eqn*', 'equation*' );
		};
		'eqnarray*' = {
			triggers = ( 'eqa*', 'eqnarray*' );
		};

		questions = {
			triggers = ( q, que, questions );
			content = "\t\\question $0";
		};
		parts = {
			triggers = ( p, par, parts );
			content = "\t\\part $0";
		};
		
		quote = {
			triggers = ( q, qu, quote );
		};
		quotation = {
			triggers = ( q, qu, quotation );
		};
		
		table = {
			triggers = ( tbl, table);
			content = "\t\\begin{center}
	\\begin{tabular}{${3:c}}
		${4}
	\\end{tabular}
	\\end{center}
	\\caption{${1:caption}}
	\\label{${2:label}}";
		};
	};
	labeled_environments = {
		theorem = {
			triggers = ( thm, theorem );
			content = "\\label{thm:$1}\n\t$0";
		};
		lemma = {
			triggers = ( lem, lemma );
			content = "\\label{lem:$1}\n\t$0";
		};
		corollary = {
			triggers = ( cor, corollary );
			content = "\\label{cor:$1}\n\t$0";
		};
		proposition = {
			triggers = ( pro, proposition );
			content = "\\label{pro:$1}\n\t$0";
		};
		definition = {
			triggers = ( def, definition );
			content = "\\label{def:$1}\n\t$0";
		};
		proof = {
			triggers = ( pf, proof );
			content = "${1:\\label{pf:$2\\}}\n\t$0";
		};

		lstlisting = {
			triggers = ( lst, lstlisting);
			content = "[caption=${1:cap},label=lst:${2:lab},float=htbp]\n\t$0";
		};
		figure = {
			triggers = ( fig, figure);
			content = "[${1:htbp}]\n\t\\centering
\t\t\\includegraphics[${2:scale=${3:1}}]{${4:file}}
\t\\caption{${5:caption}}
\t\\label{fig:${6:label}}";
		};
		picture = {
			triggers = ( pic, picture);
			content = "(${1:100},${2:100})${3:(${4:0},${5:0})}\n\t$0";
		};

	};
	environments_beamer = {
		itemize = {
			triggers = ( 'it', 'item', 'itemize');
			content = '\t\\item<${1:+-}> $2';
		};
		enumerate = {
			triggers = ( 'en', 'enum', 'enumerate');
			content = '\t\\item<${1:+-}> $2';
		};
		description = {
			triggers = ( 'desc', 'description');
			content = '\t\\item<${1:+-}>[$2] $3';
		};
		columns = {
			triggers = ( cols, columns);
			content = "\t\\begin{column}{${1:0.${2:5}\\textwidth}}
	$3
\\end{column}";
		};
		overprint = {
			triggers = (op, overprint );
			content = "\t\\onslide<${1:+}>\n\t$0";
		};
	};
	labeled_environments_beamer = {
		frame = {
			triggers = ( fr, frame );
			content = "[${1:t}]\\frametitle{${2:title}}\n\t$0";
		};
		column = {
			triggers = ( col, column );
			content = "{${1:0.${2:5}\\textwidth}}\n\t$0";
		};
		block = {
			triggers = ( bl, block );
			zero_line = "{$1}\n\t$0";
		};
		overlayarea = {
			triggers = ( oa, overlayarea );
			content = "{${1:areawidth}}{${2:areaheight}}\n\t\\only<${3:+}> $0";
		};
	};
	
