{
  "editor.fontFamily": "'MesloLGS NF', 'FiraCode Nerd Font', 'FiraCode', 'Fira Code', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 15,
  "editor.renderWhitespace": "all",
  "editor.scrollBeyondLastLine": false,
  "editor.suggestSelection": "first",
  "editor.wordWrapColumn": 79,
  "git.autofetch": true,
  "python.insidersChannel": "off",
  "python.jediEnabled": false,
  "vsintellicode.features.python.deepLearning": "enabled",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "vsonline.authentication.accountProvider": "Microsoft",
  "window.zoomLevel": 0,
  "vsonline.accountProvider": "Microsoft",
  "cmake.configureOnOpen": true,
  "codespaces.accountProvider": "Microsoft",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "html.format.endWithNewline": true,
  "editor.renderControlCharacters": true
}
