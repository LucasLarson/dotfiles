#!/usr/bin/env sh
# abbreviations

## command
abbr --global cmand='command'
abbr --global cmmand='command'
abbr --global comadn='command'
abbr --global comand='command'
abbr --global comdn='command'
abbr --global comdna='command'
abbr --global commadn='command'
abbr --global commdn='command'
abbr --global commdna='command'
abbr --global commmand='command'
abbr --global commnd='command'
abbr --global comnd='command'

## exit
abbr --global eixt='exit'

## find
abbr --global lf='exa --all --bytes --classify --color-scale --color=always --git --icons --long --no-permissions --no-user --octal-permissions --time-style=long-iso -- {} +'

## help
abbr --global ehlp='help'
abbr --global --ehlp='--help'
abbr --global hellp='help'
abbr --global --hellp='--help'
abbr --global herl='help'
abbr --global --herl='--help'
abbr --global hlep='help'
abbr --global --hlep='--help'
abbr --global hlp='help'
abbr --global --hlp='--help'

## open
abbr --global oepn='open'
abbr --global opne='open'

## zshrc
abbr --global zhsrc='zshrc'
abbr --global zsrhc='zshrc'
abbr --global zzshrc='zshrc'
